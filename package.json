    "build": "vite build"
  },
  "author": "Wes Bos",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^18.11.18",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@types/connect-history-api-fallback": "^1.3.5",
    "@wesbos/code-icons": "^1.2.3",
    "connect-history-api-fallback": "^2.0.0",
    "eslint-config-wesbos": "^3.1.5-beta.4",
    "vite": "^4.0.4",
    "vite-plugin-dts": "^1.7.1",
    "vscode-icons-js": "^11.6.1"
  }
