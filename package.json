{
  "name": "vite-plugin-list-directory-contents",
  "version": "1.4.4",
  "description": "A Vite plugin for listing the directory contents when working with the dev server",
  "homepage": "https://github.com/wesbos/vite-plugin-list-directory-contents#readme",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/plugin.cjs",
  "module": "./dist/plugin.mjs",
  "types": "./dist/plugin.d.ts",
  "exports": {
    ".": {
      "require": "./dist/plugin.cjs",
      "import": "./dist/plugin.mjs",
      "types": "./dist/plugin.d.ts"
    }
  },
  "eslintConfig": {
    "extends": [
      "wesbos/typescript"
    ]
